<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.dromara.workflow.mapper.WfFormMapper">

    <resultMap type="org.dromara.workflow.domain.vo.WfFormVo" id="WfFormResult">
    </resultMap>

    <select id="selectFormVoList" resultType="org.dromara.workflow.domain.vo.WfFormVo">
        SELECT
            t1.form_id AS formId,
            t1.form_name AS formName,
            t1.content AS content
        FROM wf_form t1
        LEFT JOIN wf_deploy_form t2 ON t1.form_id = t2.form_id
        ${ew.getCustomSqlSegment}
    </select>
</mapper>
